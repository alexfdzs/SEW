    <ruta nombre="Tanji River Paddle">
        <tipo>Paisajística</tipo>
        <medio_transporte>Canoa</medio_transporte>
        <duracion>PT4H</duracion>
        <agencia>Trekking Hiking</agencia>
        <descripcion> Explora esta ruta de ida y vuelta de 7,2-km cerca de Tujering, West Coast. Un
            corto viaje en canoa por un pequeño río que desemboca en el mar en Tanji.</descripcion>
        <personas_adecuadas> Personas en buena forma física</personas_adecuadas>
        <inicio_ruta lugar="Carretera Coastal Road" direccion="9663+27J, Coastal Rd, Tanji, Gambia">
            <coordenadas latitud="13.360393649810536" longitud="-16.796886725805148" alt_measure="m">
                8</coordenadas>
        </inicio_ruta>
        <referencias>
            <referencia>
                https://es.wikiloc.com/rutas-senderismo/tanji-river-paddle-facil-tujering-west-coast-gambia-145371314
                </referencia>
            <referencia> https://www.alltrails.com/es/ruta/the-gambia/west-coast/tanji-river-paddle </referencia>
            <referencia>
                https://www.tripadvisor.es/Attraction_Review-g3675810-d17473776-Reviews-Gambia_Kayaking-Tanji_West_Coast_Division.html
                </referencia>
        </referencias>
        <recomendacion>7</recomendacion>
        <hitos>
            <hito nombre="Tanji River Entrance">
                <descripcion>Entrada al rio Tanji</descripcion>
                <coordenadas latitud="13.360171851335481" longitud="-16.796894133622303"
                    alt_measure="m">8</coordenadas>
                <distancia dist_measure="km">0</distancia>
                <galeria_foto>
                    <foto>
                        xml/multimedia/Tanji_River_Paddle/Tanji_River_Entrance/tanji_river_entrance.png</foto>
                </galeria_foto>
            </hito>
            <hito nombre="Tanji Statue">
                <descripcion>Estatua que representa las aldeas antiguas que habitaban alrededor del
                    río Tanji</descripcion>
                <coordenadas latitud="13.362881713084317" longitud="-16.78921401225453"
                    alt_measure="m">13</coordenadas>
                <distancia dist_measure="km">16</distancia>
                <galeria_foto>
                    <foto>xml/multimedia/Tanji_River_Paddle/Tanji_Statue/tanji_statue.png</foto>
                </galeria_foto>
            </hito>
            <hito nombre="Fallen Tree">
                <descripcion>Árbol antiguo caído a la vera del río</descripcion>
                <coordenadas latitud="13.358817621816417" longitud="-16.784624582863934"
                    alt_measure="m">20</coordenadas>
                <distancia dist_measure="km">5.23</distancia>
                <galeria_foto>
                    <foto>xml/multimedia/Tanji_River_Paddle/Tanji_Fallen_Tree/tanji_fallen_tree.png</foto>
                </galeria_foto>
            </hito>
            <hito nombre="Final">
                <descripcion>Entrada al rio Tanji. lugar de final</descripcion>
                <coordenadas latitud="13.360171851335481" longitud="-16.796894133622303"
                    alt_measure="m">8</coordenadas>
                <distancia dist_measure="km">7.21</distancia>
                <galeria_foto>
                    <foto>
                        xml/multimedia/Tanji_River_Paddle/Tanji_River_Entrance/tanji_river_entrance.png</foto>
                </galeria_foto>
            </hito>
        </hitos>
    </ruta>

    <ruta nombre="Kartong">
        <tipo>Paisajística</tipo>
        <medio_transporte>A pie</medio_transporte>
        <duracion>PT2H30M</duracion>
        <agencia> Carsali</agencia>
        <descripcion> Paseo por el pueblo de Kartong, pueblo costero en el suroeste de Gambia en la
            frontera con Senegal.</descripcion>
        <personas_adecuadas> Cualquiera</personas_adecuadas>
        <inicio_ruta lugar="Restaurante Doudou's" direccion="Kartong crossing point, Gambia">
            <coordenadas latitud="13.073584406863601" longitud="-16.743567420853864" alt_measure="m">
                30</coordenadas>
        </inicio_ruta>
        <referencias>
            <referencia>https://es.wikiloc.com/rutas-a-pie/kartong-gambia-126348141</referencia>
            <referencia> https://en.wikipedia.org/wiki/Kartung </referencia>
            <referencia>
                https://www.conbotasymochila.com/viaje-a-gambia-ii-kartong-y-frontera-con-senegal/ </referencia>
        </referencias>
        <recomendacion>8</recomendacion>
        <hitos>
