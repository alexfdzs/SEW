            </hito>
            <hito nombre="Arbol pie de nius">
                <descripcion>Arbol pie de nius.</descripcion>
                <coordenadas latitud="13.073803872479798" longitud="-16.747011377111054"
                    alt_measure="m">38</coordenadas>
                <distancia dist_measure="km">0.7</distancia>
                <galeria_foto>
                    <foto>xml/multimedia/Kartong/nius_tree/nius_tree.png</foto>
                </galeria_foto>
            </hito>
            <hito nombre="Pescadores, pelicanos y buitres">
